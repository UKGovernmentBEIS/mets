{
  "name": "uk-pmrv-main-web",
  "version": "0.81.0",
  "engines": {
    "node": ">=18.19.1"
  },
  "scripts": {
    "postinstall": "node git-version.js",
    "ng": "ng",
    "start": "ng serve --serve-path /installation-aviation/",
    "start:govuk-components": "ng build govuk-components --watch --configuration development",
    "test:govuk-components": "jest --config ./projects/govuk-components/jest.config.js",
    "test:govuk-components:watch": "jest --watch --config ./projects/govuk-components/jest.config.js",
    "test:govuk-components:coverage": "jest --coverage --config ./projects/govuk-components/jest.config.js",
    "test:frontend": "jest --config ./src/jest.config.js --silent --logHeapUsage --workerIdleMemoryLimit=1GB",
    "test:frontend:watch": "jest --watch --config ./src/jest.config.js",
    "test:frontend:coverage": "jest --coverage --config ./src/jest.config.js",
    "test:app:coverage": "yarn prebuild && yarn run test:frontend:coverage",
    "test:app:watch": "yarn build:govuk-components && yarn run test:frontend:watch",
    "build:govuk-components": "ng build govuk-components",
    "build:pmrv-api": "ng build pmrv-api",
    "prebuild": "yarn build:govuk-components && yarn build:pmrv-api",
    "build": "ng build",
    "prebuild:production": "yarn build:govuk-components && yarn build:pmrv-api",
    "build:production": "ng build",
    "generate:barrels:api": "barrelsby -d projects/pmrv-api/src/lib/ -l all -D",
    "generate:prepare-swagger-file": "node.exe update-swagger-file.js",
    "pregenerate:api": "yarn generate:prepare-swagger-file && openapi-generator-cli generate -i ./projects/pmrv-api/src/assets/swagger.yaml -o projects/pmrv-api/src/lib/ -g typescript-angular -c projects/pmrv-api/openapi-config.yaml --global-property supportingFiles=.openapi-generator-ignore",
    "generate:api": "openapi-generator-cli generate -i ./projects/pmrv-api/src/assets/swagger.yaml -o projects/pmrv-api/src/lib/ -g typescript-angular -c projects/pmrv-api/openapi-config.yaml --global-property skipFormModel=false",
    "postgenerate:api": "yarn generate:barrels:api && rimraf projects/pmrv-api/src/lib/.openapi-generator && yarn build:pmrv-api && ng lint --fix && pretty-quick",
    "analyze": "ng build --source-map --named-chunks && source-map-explorer dist/uk-pmrv-main-web/*.*.js",
    "lint": "ng lint",
    "release:patch": "yarn version --patch --no-commit-hooks --no-git-tag-version && conventional-changelog -p angular -i CHANGELOG.md -s",
    "release:minor": "yarn version --minor --no-commit-hooks --no-git-tag-version && conventional-changelog -p angular -i CHANGELOG.md -s",
    "release:major": "yarn version --major --no-commit-hooks --no-git-tag-version && conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepare": "husky install",
    "sonar": "sonar-scanner",
    "sonar:local": "sonar-scanner -Dsonar.host.url=http://localhost:9000 -Dsonar.login=admin -Dsonar.password=admin"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.13",
    "@angular/common": "^18.2.13",
    "@angular/compiler": "^18.2.13",
    "@angular/core": "^18.2.13",
    "@angular/forms": "^18.2.13",
    "@angular/platform-browser": "^18.2.13",
    "@angular/platform-browser-dynamic": "^18.2.13",
    "@angular/router": "^18.2.13",
    "@hmcts/frontend": "^0.0.50-alpha",
    "@ministryofjustice/frontend": "^0.2.4",
    "@w11k/git-info": "^1.0.1",
    "@zxcvbn-ts/core": "^3.0.0",
    "@zxcvbn-ts/language-en": "^3.0.2",
    "angular-password-strength-meter": "^12.0.0",
    "bignumber.js": "9.1.1",
    "clean-deep": "^3.3.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.6.0",
    "dialog-polyfill": "^0.5.6",
    "encoding-japanese": "^2.0.0",
    "google-libphonenumber": "^3.2.10",
    "govuk-frontend": "^3.9.1",
    "immer": "^9.0.19",
    "keycloak-angular": "15.3.0",
    "keycloak-js": "25.0.1",
    "lodash-es": "^4.17.15",
    "marked": "^12.0.0",
    "ng-inline-svg-2": "^15.0.1",
    "ngx-markdown": "^18.0.0",
    "papaparse": "^5.4.1",
    "rxjs": "~7.5.6",
    "sonar-scanner": "^3.1.0",
    "strip-html-comments": "^1.0.0",
    "tslib": "^2.4.0",
    "xlsx": "^0.18.5",
    "zone.js": "^0.14.10"
  },
  "devDependencies": {
    "@angular-builders/jest": "^18.0.0",
    "@angular-devkit/build-angular": "^18.2.12",
    "@angular-eslint/builder": "^18.4.2",
    "@angular-eslint/eslint-plugin": "^18.4.2",
    "@angular-eslint/eslint-plugin-template": "^18.4.2",
    "@angular-eslint/schematics": "^18.4.2",
    "@angular-eslint/template-parser": "^18.4.2",
    "@angular/cli": "^18.2.12",
    "@angular/compiler-cli": "^18.2.13",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@testing-library/angular": "^17.3.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.5.2",
    "@types/crypto-js": "^4.2.1",
    "@types/encoding-japanese": "^2.0.4",
    "@types/google-libphonenumber": "^7.4.30",
    "@types/gtag.js": "^0.0.12",
    "@types/jest": "^29.5.13",
    "@types/lodash-es": "^4.17.11",
    "@types/marked": "^4.0.3",
    "@types/node": "^20.17.9",
    "@types/papaparse": "^5.3.7",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@typescript-eslint/types": "^8.21.0",
    "@typescript-eslint/utils": "^8.21.0",
    "axios": "^1.6.7",
    "barrelsby": "^2.2.0",
    "commit-message-validator": "^1.0.0",
    "conventional-changelog-cli": "^2.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jsdoc": "^48.2.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "fs-extra": "^10.0.0",
    "git-branch": "^2.0.1",
    "git-describe": "^4.1.0",
    "husky": "^7.0.0",
    "istanbul-combine": "^0.3.0",
    "jest": "^29.5.0",
    "jest-html-reporter": "^3.8.0",
    "jest-preset-angular": "^14.4.1",
    "js-yaml": "^4.1.0",
    "ng-packagr": "^18.2.1",
    "prettier": "^3.3.2",
    "prettier-eslint": "^16.3.0",
    "pretty-quick": "^4.0.0",
    "rimraf": "^3.0.2",
    "source-map-explorer": "^2.5.3",
    "ts-jest": "^29.1.0",
    "ts-node": "~8.10.2",
    "typescript": "~5.5.4"
  },
  "config": {
    "commit-message-validator": {
      "pattern": "^(build|ci|chore|docs|feat|fix|perf|refactor|revert|style|test)\\(([a-z0-9,;.\\-():']+)\\)\\:\\s(PMRV|METS)-[0-9]+\\s\\-\\s[a-z0-9,;.\\-():'\\s]+",
      "errorMessage": "Commit message must follow the format: feat(ui): PMRV-123 - the message goes here or feat(ui): METS-123 - the message goes here"
    }
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}